import java.util.*;
public class WeirdNumFacts
{
	public static void main(String args[])
	{
		Scanner cubee = new Scanner(System.in);
		System.out.println("Enter a number");
		int n = cubee.nextInt();
	  	int plus = n + 193;
		int minus = n - 193;
		double filterOne = Math.cbrt(plus);
		double filterTwo = Math.cbrt(minus);
		System.out.println("You typed " + n + ".");
		System.out.println(n + " + 193 = " + plus);
		System.out.println(n + " - 193 = " + minus);
		System.out.println("The cube root of " + plus + " is " + filterOne);
		System.out.println("The cube root of " + minus + " is " + filterTwo);
		//type 536 for both numbers having perfect positive cube roots :D
		}
	}